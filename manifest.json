{
	"manifest_version": 2,
	"name": "Find Resumes",
	"description": "Find guys with resumers",
	"version": "1.0",
	"permissions": [
		"https://jigsaw.thoughtworks.com/consultants/advanced_search/"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["https://jigsaw.thoughtworks.com/consultants/advanced_search/"],
			"css": ["content.css"], 
			"js": ["popup.js"]
		}
	]
}
